@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<link rel="icon" href="~/Site-Contents/img/fav-icon.png" type="image/x-icon" />
	<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
	<title>Fancy</title>

	<!-- Icon css link -->
	<link href="~/Site-Contents/css/font-awesome.min.css" rel="stylesheet">
	<link href="~/Site-Contents/vendors/elegant-icon/style.css" rel="stylesheet">
	<link href="~/Site-Contents/vendors/themify-icon/themify-icons.css" rel="stylesheet">
	<!-- Bootstrap -->
	<link href="~/Site-Contents/css/bootstrap.min.css" rel="stylesheet">

	<!-- Rev slider css -->
	<link href="~/Site-Contents/vendors/revolution/css/settings.css" rel="stylesheet">
	<link href="~/Site-Contents/vendors/revolution/css/layers.css" rel="stylesheet">
	<link href="~/Site-Contents/vendors/revolution/css/navigation.css" rel="stylesheet">
	<link href="~/Site-Contents/vendors/animate-css/animate.css" rel="stylesheet">

	<!-- Extra plugin css -->
	<link href="~/Site-Contents/vendors/owl-carousel/owl.carousel.min.css" rel="stylesheet">

	<link href="~/Site-Contents/css/style.css" rel="stylesheet">
	<link href="~/Site-Contents/css/responsive.css" rel="stylesheet">

	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
	<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
	<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->
</head>
<body>
    <!--================Search Area =================-->
    @Html.Partial("~/Views/Shared/SitePartials/Search.cshtml")
    <!--================End Search Area =================-->
    <!--================Header Menu Area =================-->
    @Html.Partial("~/Views/Shared/SitePartials/Header.cshtml")
    <!--================End Header Menu Area =================-->
    
    @RenderBody()
    
    <!--================Footer Area =================-->
    @Html.Partial("~/Views/Shared/SitePartials/Footer.cshtml")
    <!--================End Footer Area =================-->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/Site-Contents/js/jquery-3.2.1.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Site-Contents/js/popper.min.js"></script>
    <script src="~/Site-Contents/js/bootstrap.min.js"></script>
    <!-- Rev slider js -->
    <script src="~/Site-Contents/vendors/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/extensions/revolution.extension.actions.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/extensions/revolution.extension.video.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/extensions/revolution.extension.navigation.min.js"></script>
    <script src="~/Site-Contents/vendors/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <!-- Extra plugin css -->
    <script src="~/Site-Contents/vendors/counterup/jquery.waypoints.min.js"></script>
    <script src="~/Site-Contents/vendors/counterup/jquery.counterup.min.js"></script>
    <script src="~/Site-Contents/vendors/counterup/apear.js"></script>
    <script src="~/Site-Contents/vendors/counterup/countto.js"></script>
    <script src="~/Site-Contents/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Site-Contents/vendors/parallaxer/jquery.parallax-1.1.3.js"></script>
    <!--Tweets-->
    <script src="~/Site-Contents/vendors/tweet/tweetie.min.js"></script>
    <script src="~/Site-Contents/vendors/tweet/script.js"></script>

    <script src="~/Site-Contents/js/theme.js"></script>
</body>
</html>
